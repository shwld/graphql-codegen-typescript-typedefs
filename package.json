{
  "name": "graphql-codegen-typescript-typedefs",
  "version": "1.0.1",
  "repository": "git@github.com:shwld/graphql-codegen-typescript-typedefs.git",
  "author": "shwld <shu.account@outlook.com>",
  "license": "MIT",
  "volta": {
    "node": "18.12.1"
  },
  "scripts": {
    "clean": "rimraf node_modules/",
    "prebuild": "rimraf dist/ .bob/ tsconfig.tsbuildinfo",
    "build": "bob build",
    "prerelease": "yarn build",
    "release": "changeset publish"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "type": "module",
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^3.1.2"
  },
  "peerDependencies": {
    "graphql-tag": "^2.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "bob-the-bundler": "^4.1.1",
    "graphql": "^16.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  }
}
